<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.ika.pratiwiikakartika_1202154314_modul2.Menu">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="102dp"
        android:layout_height="81dp"
        app:srcCompat="@drawable/food1"
        android:onClick="food1"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="102dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_alignRight="@+id/imageView5"
        android:layout_below="@+id/imageView2"
        android:layout_marginTop="16dp"
        app:srcCompat="@drawable/food2"
        android:onClick="food1"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="106dp"
        android:layout_height="85dp"
        android:layout_alignLeft="@+id/imageView6"
        android:layout_alignStart="@+id/imageView6"
        android:layout_below="@+id/imageView6"
        android:layout_marginTop="20dp"
        app:srcCompat="@drawable/drink1"
        android:onClick="drink1"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="106dp"
        android:layout_height="98dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp"
        app:srcCompat="@drawable/drink2"
        android:onClick="drink2"/>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="102dp"
        android:layout_height="90dp"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_alignStart="@+id/imageView3"
        android:layout_below="@+id/imageView3"
        android:layout_marginTop="14dp"
        app:srcCompat="@drawable/drink3"
        android:onClick="drink3"/>


    <TextView
        android:id="@+id/hargalasagna"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lagsana"
        android:layout_alignStart="@+id/lagsana"
        android:layout_below="@+id/lagsana"
        android:layout_marginTop="11dp"
        android:text="Harga : Rp.55.000" />

    <TextView
        android:id="@+id/lagsana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView3"
        android:layout_marginTop="20dp"
        android:layout_toEndOf="@+id/imageView6"
        android:layout_toRightOf="@+id/imageView6"
        android:text="Lasagna Cheese" />

    <TextView
        android:id="@+id/mnc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/hargamnc"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/hargamnc"
        android:layout_marginTop="15dp"
        android:text="Mac and Cheese" />

    <TextView
        android:id="@+id/hargamnc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lagsana"
        android:layout_marginBottom="21dp"
        android:layout_toEndOf="@+id/imageView3"
        android:layout_toRightOf="@+id/imageView3"
        android:text="Harga : Rp.50.000" />

    <TextView
        android:id="@+id/lime"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_alignTop="@+id/imageView6"
        android:layout_marginTop="23dp"
        android:layout_toEndOf="@+id/imageView6"
        android:layout_toRightOf="@+id/imageView6"
        android:text="Lime Squash" />

    <TextView
        android:id="@+id/hargalime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:text="Harga : Rp.35.000" />

    <TextView
        android:id="@+id/soda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView4"
        android:layout_marginTop="19dp"
        android:layout_toEndOf="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:text="Soda" />

    <TextView
        android:id="@+id/hargasoda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/soda"
        android:layout_marginTop="13dp"
        android:layout_toEndOf="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:text="Harga : Rp.20.000" />

    <TextView
        android:id="@+id/red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/soda"
        android:layout_alignRight="@+id/soda"
        android:layout_alignTop="@+id/imageView5"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="12dp"
        android:text="Red Squash" />

    <TextView
        android:id="@+id/hargared"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/red"
        android:layout_marginTop="13dp"
        android:layout_toEndOf="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:text="Harga : Rp.30.000" />
</RelativeLayout>
